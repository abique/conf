<?xml version="1.0" encoding="UTF-8"?>
<shortcuts-theme name="babali">
	<shortcut command="aumix -v+10" keys="XF86AudioRaiseVolume"/>
	<shortcut command="aumix -v-10" keys="XF86AudioLowerVolume"/>
	<shortcut command="aumix -v0" keys="XF86AudioMute"/>
	<shortcut command="firefox" keys="Super+i"/>
	<shortcut command="urxvt" keys="Super+t"/>
	<shortcut command="xflock4" keys="Super+l"/>
	<shortcut command="xfrun4" keys="Super+r"/>
	<shortcut command="xkill" keys="Super+k"/>
</shortcuts-theme>
